* {
	scrollbar-width: thin;
}
:root {
	--wheat: #ffffdb;
	--dWheat: #f5dcaa;
	--Champagne: #f9ebcd;
	--2Champagne: #f3e3c2;
	--brown: #946401;
	--lbrown: #b48017;
	--white: #fefaf3;
	--2wheat: #ebbb59;
	--trans: rgba(0, 0, 0, 0.2);
	--postBtnsHover: #fdd991;
	--postclkdBtnsHover: #ebba59d8;
	--errColor:rgba(243, 121, 121, 0.945);
	font-family: "Lucida Sans";
}
html {
	scroll-behavior: smooth;
}
h1,
h2,
h3,
h4 {
	color: var(--brown);
	font-weight: 900;
	margin-top: 10px;
	margin-bottom: 10px;
	border-bottom: 1px solid var(--trans);
	border-top: 1px solid var(--trans);
	padding: 5px;
}
h1 {
}
h2 {
}
h3 {
	font-size: 2rem;
}
h4 {
	font-size: 1.5rem;
}
::-webkit-scrollbar {
	width: 5px;
	border-radius: 10px;
}
::-webkit-scrollbar-track {
	background: #f1f1f1;
	border-radius: 5px;
}
::-webkit-scrollbar-thumb {
	background: #888;
	border-radius: 5px;
}
::-webkit-scrollbar-thumb:hover {
	background: #555;
}
.err-PH::-webkit-input-placeholder{
	color:rgba(243, 121, 121, 0.945);
}
@media (max-width:400px) {
	nav  ul  div{
		display:none !important;
	}
}
body {
	background-color: var(--wheat);
}

#closeAddLibForm {
	background-color: var(--Champagne);
	float: right;
	border-radius: 5px;
	padding: 5px;
	cursor: pointer;
}
#closeAddBS{
	background-color: var(--Champagne);
	float: right;
	border-radius: 5px;
	padding: 5px;
	cursor: pointer;
}
.stores-form > ol {
	font-size: 1.5rem;
	font-weight: bold;
	list-style-type: disc;
}
.stores-form li a:link,
.stores-form li a {
	color: var(--brown);
	text-decoration: none;
}
.store-trans,
.add-lib-trans {
	background-color: var(--trans);
	position: absolute;
	width: 100vw;
	height: 100vh;
}
.add-post-btn {
	text-align: center;
	color: var(--white);
}
.add-post-btn > div {
	background-color: var(--brown);
	width: 120px;
	cursor: pointer;
	border-radius: 5px;
	display: inline-block;
}
.add-post-btn > div:hover {
	background-color: var(--lbrown);
}
.stores-c {
	position: relative;
	background-color: var(--wheat);
	padding: 10px;
	color: var(--brown);
	border-radius: 5px;
	z-index: 100;
	width: 200px;
	padding-left: 40px;
}
.post-from-c {
	position: relative;
	background-color: var(--wheat);
	padding: 10px;
	color: var(--brown);
	border-radius: 5px;
	z-index: 100;
	width: 80vw;
}

.post-btn {
	background-color: var(--brown);
	box-shadow: inset 0px -2px 2px 0px hsl(40, 80%, 35%);
	border: 0;
	padding: 20px;
	cursor: pointer;
	font-size: 1rem;
	font-weight: bold;
	text-transform: uppercase;
	color: white;
	border-radius: 10px;
	margin-top: 10px;
	float: right;
}
.post-btn:hover {
	background-color: #b48017;
	box-shadow: inset 0px -4px 2px 0px hsl(40, 80%, 35%);
}
.post-form {
}
.post-content-c {
	display: flex;
	gap: 20px;
	justify-content: space-between;
	align-items: stretch;
}
.post-content{
	max-height: 70vh;
	overflow:auto;
}
.post-content-c .post-form__book {
	flex-basis: 106px;
	width: 106px;
	min-width: 106px;
}

.post-form__book {
	width: 106px;
	height: 160px;
	margin-right: 5px;
	padding: 5px;
	background-color: var(--2wheat);
	border-radius: 5px;
	align-self: center;
}
.post-form__book > img {
	height: 100%;
	width: 100%;
	object-fit: fill;
}
.add-comment-c {
	display: flex;
	padding: 5px;
	gap: 5px;
	align-items: stretch;
}
.add-comment-c > input {
	border: 2px solid white;
}
.add-comment-btn {
	max-width: 100px;
	background-color: var(--brown);
	border-radius: 5px;
	color: var(--white);
	display: flex;
	align-items: center;
	padding: 5px;
	cursor: pointer;
}
.add-comment-btn:hover {
	background-color: var(--lbrown);
}
.add-lib-layout {
	position: fixed;
	left: 0;
	top: 0;
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100vw;
	height: 100vh;
	z-index: 200;
}
.add-lib-c {
	position: relative;
	background-color: var(--wheat);
	padding: 10px;
	color: var(--brown);
	border-radius: 5px;
	z-index: 100;
}

#closeStoresForm {
	background-color: var(--Champagne);
	float: right;
	border-radius: 5px;
	padding: 5px;
	cursor: pointer;
}
#closePostForm {
	background-color: var(--Champagne);
	float: right;
	border-radius: 5px;
	padding: 5px;
	cursor: pointer;
}

.add-lib-form {
	display: flex;
	margin-top: 25px;
	gap: 10px;
}
.add-lib-form > input[type="text"] {
	display: inline-block;
	outline: none;
}
.add-lib-form > button {
	outline: none;
	background-color: var(--brown);
	border-radius: 5px;
	color: var(--white);
}
header {
	width: 100%;
	position: fixed;
	top: 0;
	z-index: 100;
	border-bottom: 1px solid rgba(128, 128, 128, 0.13);
	background-color: #f5dcaa;
}
.header {
	display: flex;
	gap: 5px;
	padding: 0 1%;
	justify-content: space-around;
	align-items: center;
	height: 80px;
	background-color: #f5dcaa;
}
.header > * {
	position: relative;
	flex-shrink: 0;
	flex-grow: 0;
}
.top-corner-c {
	position: absolute;
	z-index: 0;
	top: 0;
	left: 0;
}

nav {
	position: relative;
	box-sizing: border-box;
	display: flex;
	z-index: -1;
	width: 100%;
	height: 80px;
	background-color: #f9ebcd;
	padding-left: 175px;
	padding-right: 10px;
}
.nav-menu {
	display: flex;
	align-items: stretch;
	flex-wrap: wrap;
	gap: 10px;
	height: inherit;
	color: var(--brown);
	min-width: 100%;
}
.nav-menu li {
	min-width: 80px;
}
.nav-menu > * {
	display: flex;
	align-items: center;
	justify-content: center;
}
.nav-menu a:link,
.nav-menu li a {
	color: var(--brown);
	text-decoration: none;
}
.lib-nav {
	box-sizing: border-box;
	display: flex;
	justify-content: start;
	align-items: center;
	width: 100%;
	height: 80px;
	padding-left: calc(min(25vw, 400px) + 30px);
	padding-right: calc(min(25vw, 400px) + 30px);
	padding-right: 10px;
	background-color: var(--wheat);
}
.logo-c {
	height: 250px;
	width: 175px;
	background-color: #f5dcaa;
	border-radius: 0 0 100% 0;
}
.logo {
	position: absolute;
	top: 75px;
	left: 5px;
	width: 100px;
	height: 100px;
}
.my-lib {
	height: 60%;
	background-color: var(--brown);
	min-width: 100px;
	display: flex;
	align-items: center;
	border-radius: 30px 5px 5px 30px;
	flex-basis: 40%;
	text-decoration: none;
}
.my-lib_menu {
	display: none;
	top: 100%;
	position: absolute;
	background-color: var(--wheat);
	width: 80%;
	box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
	padding: 12px 16px;
	list-style-type: none;
}
.my-lib_menu > * {
	color: var(--brown);
	cursor: pointer;
}
.my-lib:hover .my-lib_menu {
	display: block;
}
.my-lib:hover {
	background-color: hsl(40, 99%, 29%, 0.9);
}

.my-lib__img {
	height: 40px;
	width: 40px;
	border-radius: 50%;
	border: 1px solid var(--white);
	margin-left: 2px;
	object-fit: cover;
	object-position: center center;
}
.my-lib__name {
	padding: 10px;
	color: var(--white);
	overflow: hidden;
	text-overflow: ellipsis;
}

.search-icon {
	box-sizing: border-box;
	position: relative;
	display: block;
	width: 20px;
	height: 20px;
	border: 2px solid;
	border-radius: 100%;
	margin-left: -4px;
	margin-top: -4px;
	border-color: var(--brown);
}

.search-icon::after {
	content: "";
	display: block;
	box-sizing: border-box;
	position: absolute;
	border-radius: 3px;
	width: 3px;
	height: 10.5px;
	background: currentColor;
	transform: rotate(-45deg);
	top: 13px;
	left: 15px;
	background-color: var(--brown);
}
.m-search {
	display: grid;
	grid-template-columns: 4fr 100px;
	align-items: center;
	gap: 10px;
	position: absolute;
	width: calc(100% - 175px);
	height: 100%;
	background-color: var(--Champagne);
}
.algin-left {
	justify-self: left;
}
.algin-right {
	justify-self: right;
}
.m-search-btn {
	width: 10px;
	height: 10px;
	padding: 0;
}
.search-input {
	outline: none;
	height: 20px;
	width: 90%;
}
.notifications {
	position: relative;
}
.notifications-red-dot{
	position: absolute;
	border-radius:50%;
	right:20px;
	top:20px;
	cursor: pointer;
}
.notifications-red-dot::before{
	position: absolute;
	top:50%;
	left:50%;
	transform: translate(-50%,-50%);
	border-radius:50%;
	content: "";
	width:13px;
	height: 13px;
	border:2px solid tomato;
}
.notifications-red-dot::after{
	position: absolute;
	top:50%;
	left:50%;
	transform: translate(-50%,-50%);
	border-radius:50%;
	content: "";
	width:10px;
	height: 10px;
	background:tomato;

}
.bell {
	width: 30px;
	height: 30px;
	padding: 10px;
	border-radius: 5px;
	cursor: pointer;
}

.bell img {
	width: 100%;
}
.notifications-c {
	position: absolute;
	width: 70vw;
	top: 100%;
	right: -30vw;
	height: calc(65px * 5);
	background-color: var(--wheat);
	border: 3px solid rgba(128, 128, 128, 0.13);
	overflow: auto;
	padding: 10px;
	display: flex;
	flex-direction: column;
	gap: 10px;
	border-radius: 5px;
	box-sizing: border-box;
}
.notif-slab {
	display: flex;
	justify-content: start;
	align-items: center;
	gap: 10px;
	background-color: var(--Champagne);
	padding: 5px;
	border-radius: 90px 20px 20px 90px;
	border: 1px solid var(--white);
	color: var(--brown);
	text-decoration: none;
}
.notif-slab:link {
	color: var(--brown);
	text-decoration: none;
}
.notif-slab:hover {
	opacity: 0.8;
}
.notif-img {
	min-width: 40px;
	min-height: 40px;
	width: 40px;
	height: 40px;
	border-radius: 50%;
	overflow: hidden;
	border: 1px solid var(--white);
	box-shadow: 0px 0px 10px 3px var(--brown);
}
.notif-img > img {
	height: inherit;
	width: inherit;
	object-fit: cover;
	object-position: center center;
}
.notif-name {
	font-weight: bold;
}
.s-search {
	display: none;
}
._clicked {
	background-color: var(--Champagne);
}
.menu,
.search-btn,
.s-search-btn {
	display: flex;
	justify-content: center;
	position: relative;
	align-items: center;
	width: 40px;
	height: 40px;
	padding: 5px;
	border-radius: 5px;
	cursor: pointer;
}
.menu:not(._clicked),
.search-btn:not(._clicked),
.bell:not(._clicked) {
	background-color: #e7c98e;
}
/*menu*/
.menu {
	position: relative;
	z-index: 100;
}

.bar1,
.bar2,
.bar3 {
	width: 35px;
	height: 5px;
	background-color: var(--brown);
	margin: 6px 0;
	transition: 0.4s;
	border-radius: 20%;
}

.change .bar1 {
	-webkit-transform: rotate(-45deg) translate(-9px, 6px);
	transform: rotate(-45deg) translate(-9px, 6px);
}

.change .bar2 {
	opacity: 0;
}

.change .bar3 {
	-webkit-transform: rotate(45deg) translate(-8px, -8px);
	transform: rotate(45deg) translate(-8px, -8px);
}
.chat-side {
	display: flex;
	justify-content: end;
	position: fixed;
	z-index: 5;
	height: 100vh;
	top: 80px;
	right: 0;
	width: 100vw;
	padding-top: 80px;
}
.chat-frame {
	border-left: 1px solid rgba(128, 128, 128, 0.13);
	display: flex;
	flex-direction: column;
	background-color: var(--wheat);
	z-index: 1;
	position: absolute;
	width: 80%;
	padding-left: 10px;
	padding-right: 10px;
	transition: all 0.3s ease-out;
}
.hide-chat .chat-frame {
	margin-right: -100%;
}
.chat-side:not(.hide-chat) .trans {
	background-color: var(--trans);
}
.chat {
	display: grid;
	grid-template-rows: 1fr;
	position: relative;
	gap: 10px;
	padding: 10px;
	border: 1px solid var(--trans);
	border-bottom: unset;
}
.chat > * {
	max-width: 97%;
	min-width: 97%;
	width: 97%;
}
.chatted-lib {
	margin-top: 75px;

	padding: 10px 0 0 0;
	color: var(--brown);
}
.chatted-lib > a:link {
	text-decoration: none;
}
.chatted-lib > a {
	display: flex;
	align-items: center;
	gap: 10px;
	background-color: var(--dWheat);
	padding: 5px;
	border-radius: 20px 20px 0 0;
	color: var(--brown);
}
.chatted-lib > a > div {
	max-width: 50px;
	max-height: 50px;
	border-radius: 50%;
	box-shadow: 0px 0px 10px 0px var(--brown);

	overflow: hidden;
}
.chatted-lib > a > div > img {
	width: 50px;
	height: 50px;
	box-sizing: border-box;
	object-fit: cover;
	border-radius: 50%;

	border: 1px solid var(--white);
}
.chat-c {
	display: flex;
	flex-direction: column-reverse;
	overflow: auto;
}
.chat__row {
	padding: 1px;
}
.chat__u1 .chat__msg-c {
	float: right;
	max-width: 70%;
}
.chat__u2 .chat__msg-c {
	float: left;
	max-width: 70%;
}
.chat__msg {
	width: fit-content;
	border-radius: 20px;
	padding: 5px 10px;
	white-space: pre-wrap;
	word-break: break-word;
	word-wrap: break-word;
}
.chat__time {
	font-size: 0.7rem;
	margin-top: 2px;
	color: gray;
	clear: both;
}
.chat__u1 .chat__time {
	text-align: left;
	margin-left: 10px;
}
.chat__u2 .chat__time {
	text-align: right;
	margin-right: 10px;
}

.chat__u1 .chat__msg {
	float: right;
	background-color: var(--brown);
	color: var(--white);
}
.chat__u2 .chat__msg {
	background-color: var(--Champagne);
	color: var(--brown);
	float: left;
}
.chat__footer {
	height: fit-content;
	display: flex;
	gap: 10px;
	padding: 5px;
	align-items: center;
	justify-content: space-between;
	border: 1px solid var(--trans);
	border-radius: 5px;
}
.chat__txtarea {
	max-height: 200px;
	overflow: auto;
}
.send-chat {
	width: 0;
	height: 0;
	border-top: 17px solid transparent;
	border-bottom: 17px solid transparent;
	border-left: 30px solid var(--brown);
	border-radius: 100%;
}
.send-chat:hover {
	border-left: 30px solid var(--lbrown);
}
.lib-list {
	position: fixed;
	height: 100vh;
	top: 80px;
	width: 100vw;
	padding-top: 80px;
	z-index: 5;
}
.hide-list .lib-list-frame {
	margin-left: -100%;
}
.lib-list:not(.hide-list) .trans {
	background-color: var(--trans);
}
.lib-list-frame {
	border-right: 1px solid rgba(128, 128, 128, 0.13);
	display: flex;
	flex-direction: column;
	gap: 40px;
	background-color: var(--wheat);
	z-index: 1;
	position: absolute;
	width: 80%;
	padding-left: 10px;
	padding-right: 10px;
	transition: all 0.3s ease-out;
}
.list {
	display: flex;
	flex-direction: column;
	gap: 10px;
	overflow: auto;
	padding: 10px;
	margin-top: 80px;
}
.list > * {
	flex-grow: 0;
}
.list-header {
	width: 100%;
}
.list-header > * {
	float: left;
}
.add-lib-icon {
	position: absolute;
	top: 160px;
	left: 40px;
	cursor: pointer;
	font-size: 50px;
	font-weight: 700;
	color: var(--brown);
}

.lib {
	position: relative;
	cursor: pointer;
	height: 46px;
	border: 1px solid black;
	border-radius: 5px;
	display: flex;
	align-items: center;
	padding-right: 30px;
	box-shadow: 3px 3px 10px var(--brown);
}
.lib a:link {
	color: var(--brown);
	text-decoration: none;
}
.lib-frame {
	display: flex;
	justify-content: start;
	align-items: center;
	gap: 10px;
	width: calc(100% - 10px);
}
.lib_pic {
	height: 40px;
	width: 40px;
	border-radius: 5px;
	border: 2px solid var(--brown);
	margin-left: 2px;
	object-fit: cover;
}
.lib_name {
	max-width: calc(100% - 50px);
	color: var(--brown);
	overflow: hidden;
	text-overflow: ellipsis;
}
.lib:hover {
	background-color: #fefaf34f;
}
.chat-btn-c {
	position: absolute;
	right: 10px;
}
.chat-btn-p1 {
	width: 30px;
	height: 20px;
	background-color: var(--brown);
	border-radius: 4px;
}
.chat-btn-p2 {
	content: "";
	display: block;
	position: absolute;
	width: 0;
	height: 0;
	right: 10px;
	bottom: -9px;
	border-left: 10px solid transparent;
	border-right: 2px solid transparent;
	border-top: 10px solid var(--brown);
}
.chat-btn-p1:hover {
	background-color: #ad7a13;
}
.chat-btn-p1:hover > .chat-btn-p2 {
	border-top: 10px solid #ad7a13;
}
.display {
	display: none !important;
}
.trans {
	z-index: 0;
	transition: all 0.5s ease;
	position: absolute;
	width: 100%;
	height: 100%;
}
main {
	padding: 10px;
}
.root {
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 10px;
}
.community {
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 10px;
}
.post {
	width: 90%;
	background-color: var(--Champagne);
	padding: 10px;
	border-radius: 10px;
}
.post > a {
	display: inline-block;
	width: 50%;
}
.post__lib {
	display: flex;
	justify-content: start;
	align-items: center;
	gap: 10px;
	border: 1px solid var(--trans);
	border-radius: 10px;
	padding: 5px;
}
.post__lib:hover {
	background-color: var(--2Champagne);
}
.post__lib-img {
	width: 40px;
	height: 40px;
	object-fit: cover;
	border-radius: 50%;
	border: 2px solid var(--brown);
}
.post__name {
	color: var(--brown);
	overflow: hidden;
	text-overflow: ellipsis;
}
.post__content-slab {
	margin: 10px 0;
	display: flex;
}
.post__content {
	font-size: 1.15rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: pre-wrap;
	word-break: break-word;
	word-wrap: break-word;
	padding: 10px;
	border-radius: 10px;
	margin-bottom: 10px;
}
.post__bar {
	height: 20px;
	border: 1px solid var(--trans);
	border-radius: 5px;
	clear: both;
	padding: 3px;
}
.post > a:link {
	text-decoration: none;
}
.post__menu {
	clear: both;
	height: 50px;
	display: flex;
	align-items: stretch;
	justify-content: center;
	flex-direction: column;
	gap: 10px;
	flex-wrap: wrap;
	border: 1px solid var(--trans);
	padding: 5px;
	border-radius: 5px;
	margin-top: 10px;
}
.post__menu-icon {
	padding: 2px;
	background-color: var(--dWheat);
	border-radius: 5px;
	text-align: center;
}
.post__menu-icon:hover._post-btn-clicked {
	background-color: var(--postclkdBtnsHover);
}
.post__menu-icon:hover {
	background-color: var(--postBtnsHover);
}
.post__menu-icon > img {
	width: 20px;
	padding: 10px;
}
.post__date,.post__date:link {
	text-decoration: none;
	color:#000;
	float: right;
}
.post__date:hover{
	text-decoration: underline;
}
._post-btn-clicked {
	background-color: var(--2wheat);
}
.comments-slab {
	overflow: auto;
}

